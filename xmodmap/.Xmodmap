clear Lock
keysym Caps_Lock = Escape
keysym Escape = Caps_Lock
add Lock = Caps_Lock

keysym Insert = Return
keysym Home = comma
keysym Prior = period


! remove shift = Shift_R
! remove control = Control_R
! remove mod4 = Super_R

! keysym Shift_R = Up
! keysym Super_R = Left
! keysym Control_R = Right
! keysym Menu = Down

! keysym Scroll_Lock = 2
! keysym Pause = 3
! keysym Delete = 7
! keysym End = 8
! keysym Next = 9

! keycode 107 = 1
